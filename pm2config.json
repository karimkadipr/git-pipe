{
  "apps": [{
    "name": "api",
    "exec_mode": "cluster",
    "instances": "max",
    "script": "yarn",
    "args": "start:prod",
    "interpreter": "node",
    "watch": ["./dist"],
    "env": {
      "NODE_ENV": "development"
    },
    "env_production": {
      "NODE_ENV": "production"
    },
    "log_date_format": "DD-MM-YYYY HH:mm Z",
    "error_file": "pm2_api_err.log",
    "out_file": "pm2_api_out.log"
  }]
}